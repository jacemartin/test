{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "https://visa.ci.modirum.com/mdpayacs-admin/main/main.htm"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Transactions"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "txButton"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='searchOptions']/input[3]"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='searchOptions']/input[4]"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='searchOptions']/input[5]"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='searchOptions']/input[6]"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//table[@class='halfSecond']/tbody/tr[4]/td[2]/input[3]"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "input.blueButton"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "Your search results exceed the display limit. The most recent 1,000 transactions are displayed,"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}